{
  "name": "@hg-ui/<%= name %>",
  "version": "0.0.0",
  "description": "<%= description %>",
  "scripts": {
    "bootstrap": "pnpm install",
    "start": "npm run bootstrap && npm run dev",
    "dev": "hg-cli preview",
    "build": "hg-cli build",
    "lintcss": "stylelint \"src/**/*.scss\" --fix",
    "lint": "eslint --ext .js,.vue src",
    "lintfix": "eslint --fix --ext .js,.vue src",
    "cz": "git add . && git cz",
    "pub": "npm run bootstrap && npm run lint && sh build/git-release.sh && sh build/release.sh"
  },
  "keywords": [
    "vue",
    "component",
    "frame",
    "page"
  ],
  "author": "<%= author %>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/WenHaoHuang/h-<%= name %>.git"
  },
  "homepage": "",
  "bugs": {
    "url": "https://github.com/WenHaoHuang/h-<%= name %>/issues"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./.czconfigrc.js"
    }
  },
  "engines": {
    "node": ">= 8.9.1",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "peerDependencies": {
    "vue": "3.2.26",
    "element-plus": "1.3.0-beta.7",
    "@element-plus/icons-vue": "0.2.6",
    "vue-router": "4.0.12"
  },
  "dependencies": {
    "vue": "3.2.26",
    "element-plus": "1.3.0-beta.7",
    "@element-plus/icons-vue": "0.2.6",
    "@vue/shared": "3.2.29",
    "lodash": "4.17.21",
    "typescript": "4.5.5",
    "autoprefixer": "10.4.2",
    "postcss": "8.4.5",
    "postcss-import": "14.0.2",
    "postcss-url": "10.1.3",
    "sass": "1.49.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.10.0",
    "@typescript-eslint/parser": "5.10.0",
    "eslint": "8.7.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-vue": "8.3.0",
    "stylelint": "14.3.0",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-recess-order": "3.0.0",
    "stylelint-config-standard": "24.0.0",
    "stylelint-order": "5.0.0",
    "stylelint-scss": "4.1.0"
    "select-version-cli": "0.0.2"
  }
}
